import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;
@Configuration

public class StudentConfig
{
    @Bean
    public LoanInterface()
    {
        Student bean = new Student();
        bean.getLoanAmount((LoanAmount));
        return bean;
    }

    @Bean
    public UnderGrad()
    {
       new UnderGrad bean = new UnderGard();
        return bean;
     public double Interest()
    {
        return super.getLoanAmount()*0.12;
    }
    
    }
    
   @Bean
   public PostGrad()
   {
   
   new PostGrad bean = new PostGard();
   return bean;
   
   Pubilc double Interest()
   {
       return super.getLoanAmount()*0.10;
   }
   
   }
   
   @Bean
   public SmallBusiness()
   {
   
   new SmallBusiness bean = new SmallBusiness();
   return bean;
   
   public double Interest()
   {
      return LoanAmount* 0.15;
   }
   
  } 

}
